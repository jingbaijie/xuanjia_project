	<div style="position: absolute;width: 38px;height: 38px; left: 78px;top:37px;background:url(<%= localImg %>searchIcon.png) no-repeat">
	</div>
	<!--搜索输入框-->
	<div class="userTextDiv">
		<span id="userText"></span>
		<span id="userText1" style="position: absolute;left: 0px;top: 0px;">请输入节目首字母</span>
	</div>
	<!--删除按钮-->
	<div class="btn" id="hands_x0_y0_del_"
		style="width:104px;height:48px;top:95px;left:78px;background:url(<%= localImg %>delete.png) no-repeat">
