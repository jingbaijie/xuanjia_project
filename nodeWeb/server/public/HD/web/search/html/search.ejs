	</div>
	<!--清空按钮-->
	<div class="btn" id="hands_x0_y0_clear_"
		style="width:104px;height:48px;top:95px;left:215px;background:url(<%= localImg %>clear.png) no-repeat">
		<div id="clear"
			style="visibility: hidden;;width:104px;height:48px;background:url(<%= localImg %>clearBorder.png) no-repeat">
		</div>
	</div>
	<!-- 文字 -->
	<div id="allSearch">
		<img style="float:left;" src="<%= localImg %>guessYouLike.png"/>
		<div style="margin-left:10px;float:left;font-size: 20px;">猜你喜欢</div>
	</div>
	<div id="searchAll">
	</div>
	
	<!--26个字母-->
	<div id="letter" class="numberDiv" style="visibility: visible;">
        <%for (var i = 0; i < 26; i++) {%>
            <div class='btn letterBj numberOrletter' id="hands_x0_y0_letter<%=i%>_" 
                style="top:<%= searchInfo.letterPosition[i].top %>px;left:<%= searchInfo.letterPosition[i].left %>px;">
                <div class='letterBorder' id="letter<%= i %>"></div>
                <div class='btn letterTextDiv'>
                    <span id="letterSpan<%= i %>"><%= searchInfo.letterPosition[i].value %></span>
                </div>
            </div>
        <%}%>
    </div>
    <!--10个数字-->
	<div id="number" class="numberDiv" style="visibility: hidden;">
        <%for (var i = 0; i < 10; i++) {%>
            <div class='btn numberBj numberOrletter' id="hands_x0_y0_number<%= i %>_" 
                style="top:<%= searchInfo.numberPosition[i].top %>px;left:<%= searchInfo.numberPosition[i].left %>px;">
                <div class='btn numberBorder' id="number<%= i %>"></div>
                <span class='btn' id="numberSpan<%= i %>" style='top:19px;left:32px;'><%= i %></span>
            </div>
        <%}%>
    </div>
	
	<!--数字字母切换按钮-->
	<div class="btn" id="hands_x0_y0_changeNumber_"
		style="top:624px;left:50px;width:150px;height:66px;background:url(<%= localImg %>numberBottom.png)">
		<div id="changeNumber"
			style="visibility: hidden;width:150px;height:66px;background:url(<%= localImg %>numberLetterB.png)"></div>
	</div>
	<div class="btn" id="hands_x0_y0_changeLetter_"
		style="top:624px;left:185px;width:150px;height:66px;background:url(<%= localImg %>letterBottom.png)">
		<div class="btn" id="changeLetter"
			style="visibility: hidden;width:150px;height:66px;background:url(<%= localImg %>numberLetterB.png)"></div>
	</div>
	<!--中间热搜-->
	<div id="hotSearch"></div>
	<!--右边搜索结果 先显示6个固定的推荐图片-->
	<div id="recommend"></div>
		<img  id="downArrow" src="<%= localImg %>down.png">
	</div>

	<div style="width: 200px;height: 60px;position: absolute;left: 400px;top:42px">
		<img style="float: left"  src="<%= localImg %>hotIcon.png"/>
		<div style="float: left;margin-left: 10px;margin-top: 5px;"><span style="font-size: 20px;color: white;font-weight: bold;">大家都在看</span></div>
	</div>
    <%- include('../../components/commJs') %>
