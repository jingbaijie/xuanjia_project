            function trimTime(timeMin) {
                if (timeMin) {
                    var dateTime = new Date(timeMin);
                    return dateTime.getFullYear() + "" +
                        (dateTime.getMonth() > 10 ? (dateTime.getMonth() + 1) : ("0" + (dateTime.getMonth() + 1))) +
                        (dateTime.getDate() > 10 ? dateTime.getDate() : ("0" + dateTime.getDate())) +
